    </listOfProducts>
    <kineticLaw metaid="_182259">
      <notes>
        <body xmlns="http://www.w3.org/1999/xhtml">
          kf*PKA_slash_R2C2*cAMP-kb*PKA_slash_cAMP_dot_R2C2</body>
      </notes>
      <math xmlns="http://www.w3.org/1998/Math/MathML">
        <apply>
          <minus/>
          <apply>
            <times/>
            <ci> kf </ci>
            <ci> PKA_slash_R2C2 </ci>
            <ci> cAMP </ci>
          </apply>
          <apply>
            <times/>
            <ci> kb </ci>
            <ci> PKA_slash_cAMP_dot_R2C2 </ci>
          </apply>
        </apply>
      </math>
